{
  "openwebui": {
    "url": "http://test:8080",
    "api_key": "test_key"
  },
  "sync": {
    "schedule": "daily",
    "time": "00:00",
    "day": "0",
    "timezone": "UTC"
  },
  "files": {
    "directory": "/data",
    "allowed_extensions": [
      ".md",
      ".txt",
      ".pdf",
      ".doc",
      ".docx",
      ".json",
      ".yaml",
      ".yml"
    ],
    "state_file": "/app/sync_state.json"
  },
  "knowledge_bases": {
    "single_kb_mode": false,
    "single_kb_name": "",
    "mappings": []
  },
  "retry": {
    "max_attempts": 3,
    "delay": 60,
    "upload_timeout": 300
  },
  "ssh": {
    "enabled": false,
    "key_path": "/app/ssh_keys",
    "strict_host_key_checking": false,
    "sources": []
  },
  "volumes": []
}